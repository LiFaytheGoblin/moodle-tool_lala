{{!
    @template tool_laaudit/evidence_set
    Template for evidence_set.
    Context variables required for this template:
    - id: 1
    - name: "training data"
    - timecreationstarted: "1679987403"
    - timecreationfinished: "1679987953"
    - serializedfilelocation:
    Example context (json): "/evidencecollections/1"
    {
        "id": 1,
        "name": "training data",
        "timecreationstarted": "1679987403",
        "timecreationfinished": "1679987953",
        "serializedfilelocation": "/evidencecollections/1"
      }
}}
<h5>evidence{{id}}</h5>
<ul class="description p-0 list-unstyled">
    <li class="course-description-item config-info"><strong>{{#str}}name, tool_laaudit{{/str}}</strong> {{name}}</li>
    <li class="course-description-item config-info"><strong>{{#str}}started, tool_laaudit{{/str}}</strong> {{timecreationstarted}}</li>
    <li class="course-description-item config-info"><strong>{{#str}}finished, tool_laaudit{{/str}}</strong> {{timecreationfinished}}</li>
    <li class="course-description-item config-info"><strong>{{#str}}serializedfilelocation, tool_laaudit{{/str}}</strong> {{serializedfilelocation}}</li>
</ul>